geany:
	geany $(wildcard *.c *.h)
atom:
	atom $(wildcard *.c *.h)
sublime:
	subl $(wildcard *.c *.h)
emacs:
	emacs $(wildcard *.c *.h)
vim:
	vim $tabe: main.c
	vim $tabe: googlebot_ds.c
	vim $tabe: googlebot_lists.c
	vim $tabe: googlebot_csv.c
	vim $tabe: googlebot.h
gedit:
	gedit $(wildcard *.c *.h)
mousepad:
	mousepad $(wildcard *.c *.h)
